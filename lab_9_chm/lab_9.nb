         RowBox[{"difference", "=", 
          RowBox[{"tmpRoot", "-", "root"}]}], ";", "\[IndentingNewLine]", 
         RowBox[{"If", "[", 
          RowBox[{
           RowBox[{"difference", "<", "0"}], ",", 
           RowBox[{"difference", "=", 
            RowBox[{"-", "difference"}]}]}], "]"}], ";", 
         "\[IndentingNewLine]", 
         RowBox[{"Print", "[", 
          RowBox[{
          "\"\<iteration: \>\"", ",", "i", ",", "\"\<  root: \>\"", ",", 
