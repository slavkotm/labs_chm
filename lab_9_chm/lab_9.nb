      RowBox[{"While", "[", 
       RowBox[{
        RowBox[{"difference", "\[GreaterEqual]", "epsi"}], ",", 
        "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{"tmpRoot", "=", "root"}], ";", "\[IndentingNewLine]", 
         RowBox[{"root", "=", 
          RowBox[{"root", "-", 
