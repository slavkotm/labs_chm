 RowBox[{
  RowBox[{
   RowBox[{"findRoots", "[", 
    RowBox[{"epsi_", ",", "x0_"}], "]"}], ":=", 
   RowBox[{"Block", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
       RowBox[{"difference", "=", "epsi"}], ",", 
       RowBox[{"i", "=", "0"}], ",", 
       RowBox[{"root", "=", "x0"}], ",", "tmpRoot"}], "}"}], ",", 
     "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"While", "[", 
       RowBox[{
        RowBox[{"difference", "\[GreaterEqual]", "epsi"}], ",", 
        "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{"tmpRoot", "=", "root"}], ";", "\[IndentingNewLine]", 
         RowBox[{"root", "=", 
          RowBox[{"root", "-", 
           RowBox[{
            RowBox[{"(", 
             RowBox[{
              SuperscriptBox["root", "4"], "-", 
              RowBox[{"2.9", "*", 
               SuperscriptBox["root", "3"]}], "+", 
              RowBox[{"0.1", "*", 
               SuperscriptBox["root", "2"]}], "+", 
              RowBox[{"5.8", "*", "root"}], "-", "4.2"}], ")"}], "/", 
            RowBox[{"(", 
             RowBox[{
              RowBox[{"4", "*", 
               SuperscriptBox["root", "3"]}], "-", 
              RowBox[{"8.7", "*", 
               SuperscriptBox["root", "2"]}], "+", 
              RowBox[{"0.2", "*", "root"}], "+", "5.8"}], ")"}]}]}]}], ";", 
         "\[IndentingNewLine]", 
         RowBox[{"difference", "=", 
          RowBox[{"tmpRoot", "-", "root"}]}], ";", "\[IndentingNewLine]", 
         RowBox[{"If", "[", 
          RowBox[{
           RowBox[{"difference", "<", "0"}], ",", 
           RowBox[{"difference", "=", 
            RowBox[{"-", "difference"}]}]}], "]"}], ";", 
         "\[IndentingNewLine]", 
         RowBox[{"Print", "[", 
          RowBox[{
          "\"\<iteration: \>\"", ",", "i", ",", "\"\<  root: \>\"", ",", 
           "root", ",", "\"\<  accuracy: \>\"", ",", "difference"}], "]"}], 
         ";", "\[IndentingNewLine]", 
         RowBox[{"i", "+=", "1"}], ";"}]}], "\[IndentingNewLine]", "]"}], ";",
       "\[IndentingNewLine]", 
      RowBox[{"Return", "[", "root", "]"}], ";"}]}], "\[IndentingNewLine]", 
    "]"}]}], ";"}]], "Input",
